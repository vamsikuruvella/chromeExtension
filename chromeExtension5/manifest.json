{
    "manifest_version":2,
  
    "name":"Getting Started",
    "description":"This extension show",
    "version":"1.0",
  
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "icons":{
        "64":"icon.png"
    },
    "background":{
        "scripts":["background.js"]
    },
    "content_scripts": [
        {
          "js": [
            "myscript.js"
          ],
          "matches": [
            "http://*/*"
          ]
        }
      ]
  }