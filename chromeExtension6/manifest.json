{
    "manifest_version": 2,
    "name": "OpenStreet Maps",
    "description": "View maps inside this extension",
    "version": "1.0",
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  